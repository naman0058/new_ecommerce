                                                    <th>Cancel</th>
                                                <th>Edit Order</th>

    
                                                    <% } else { %>
                                                        <th>Last Update</th>
    
    
                                                        <% } %>
                                                    <th>View Details</th>

    
                                            </tr>
                                        </tfoot>
                                    
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                   
                </div>
