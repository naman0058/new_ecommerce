            Content body start
        ***********************************-->
        <div class="content-body">

            <div class="row page-titles mx-0">
               
              
            </div>
            <!-- row -->

            <div class="container-fluid">
                
              
                <div class="row">
                    <div class="col-12">
                        <h3 class="content-heading"><%=title%></h3>
                    </div>
                    <div class="col-12">
                        <div class="card">
                            <div class="card-body">
                                <div class="table-responsive">
        <button type="button" class="btn btn-primary float-right" id="exportBtn" style="float: right;margin-bottom:20px">Export Data</button>

                                    <table id="example" class="display" style="min-width: 845px">
                                        <thead>
                                            <tr>

                                                <th>OrderID</th>
                                                                                                <th>Name</th>
                                                <th>Number</th>
                                                <th>Address</th>
                                                <th>Quantity</th>
                                                <th>Price</th>
                                                <th>Date</th>
                                                <th>Agent</th>
                                                <% if(msg == 'running'){ %>

                                                <th>Complete</th>
                                                <th>Cancel</th>
                                                <th>Edit Order</th>


                                                <% } else { %>
                                                    <th>Last Update</th>


                                                    <% } %>
                                                    <th>View Details</th>

                                            </tr>
                                        </thead>
                                        <tbody>

<% for(i=0;i<result.length;i++) { %>

                                            <tr>
                                                <td><%=result[i].orderid%></td>
                                              
                                                <td><%=result[i].name%></td>
                                                <td><%=result[i].number%></td>
                                                <td><%=result[i].address%></td>
                                                <td><%=result[i].items%></td>
                                                <td><%=result[i].price%></td>
                                                <td><%=result[i].date%></td>
                                                <td><%=result[i].agentname%></td>
                                                <% if(msg == 'running'){ %>
                                                <td><a href="/agent/dashboard/update/booking-status?orderid=<%=result[i].orderid%>"><button type="button" class="btn btn-success">Complete</button></a></td>
                                                <td><a href="/agent/dashboard/update/booking-status?orderid=<%=result[i].orderid%>"><button type="button" class="btn btn-danger">Cancel</button></a></td>
                                                <td><a href="/agent/dashboard/get-orders?orderid=<%=result[i].orderid%>"><button type="button" class="btn btn-danger">Edit Order</button></a></td>

                                                <% } else { %>
                                                    <td><%=result[i].updated_date%></td>


                                                    <% } %>


                                                <td><a href="/agent/dashboard/view-order?orderid=<%=result[i].orderid%>"><button type="button" class="btn btn-link">View Order</button></a></td>
                                            </tr>

                                            <% } %>
                                            
                                        </tbody>
                                        <tfoot>
                                            <tr>
                                                <th>OrderID</th>
                                            
                                                <th>Name</th>
                                                <th>Number</th>
                                                <th>Address</th>
                                                <th>Quantity</th>
                                                <th>Price</th>
                                                <th>Date</th>
                                                <th>Agent</th>
                                                <% if(msg == 'running'){ %>

